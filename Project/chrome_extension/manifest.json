{
  "manifest_version": 3,
  "name": "YARA Malware Detector",
  "version": "1.0",
  "description": "다운로드한 파일을 YARA 룰로 검사하는 Chrome 확장 프로그램",
  "permissions": ["downloads", "notifications"],
  "host_permissions": ["http://localhost:5000/*"],
  "background": {
    "service_worker": "background.js"
  },
  "icons": {
    "48": "icons/alert.png",
    "128": "icons/vaccine.png"
  }
}
