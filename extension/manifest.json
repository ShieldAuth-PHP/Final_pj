{
  "manifest_version": 3,
  "name": "Malware Scanner",
  "version": "1.0",
  "description": "Detects Emotet, SocGholish, and IcedID malware",
  "permissions": [
    "declarativeNetRequest",
    "storage",
    "activeTab",
    "downloads",
    "notifications"
  ],
  "host_permissions": [
    "<all_urls>"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icons/alert.png",
      "48": "icons/vaccine.png"
    }
  },
  "icons": {
    "16": "icons/alert.png",
    "48": "icons/vaccine.png"
  }
}
